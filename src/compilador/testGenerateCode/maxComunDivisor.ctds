class maxComunDivisor{

int maxComun(int a, int b){ // asumiendo que a es mayor que b --a>b 
	int mcd; 
	int i;
	mcd=1;
	for i=1, b { 
		if ( a%i == 0 && b%i==0){
			mcd=i; 
		}
	} 
	return mcd; // retorna el maximos comun divisor 
	} 
	


int main(){
	int a,b;
	a=6;
	b=4;
    externinvk("printf", void , "%d\n",maxComun(a,b));
}

}