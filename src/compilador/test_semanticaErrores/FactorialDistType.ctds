/*en este test, la funcion factorial tomara un int, pero desde main le pasaremos a, que es de tipo float, por lo tanto debe fallar
*/
class FactorialDistType{

int factorial(int n){

	int i;
	int total;
        i = n;
        total = 1;
	while (i > 0 ){
		total = total * i;
		i = i - 1;
	}

	return total;
}

int main(){
    float a;
	a=10;
    externinvk("printf", int , factorial(a)); //a es tipo float y factorial espera un int
}

}
