/*prueba general de integracion, en la cual comprobamos el pasaje de parametros, los tipos de asignacion, el test deberia pasar*/

class grados{

	float aFahrenheit (float celcius){
		float result;
		result = (celcius * 1.8);
		result +=32.0;
		return result;
	}

	float aCelcius (float f){
		float result;
		f -= 32.0;
		result = f/1.8;
		return result;
	}

	void main(){
		float c;
		float d;
		c=28.0;
		d=30.0;
		d=aCelcius(d);
		externinvk("printf",float,aFahrenheit(c));
		externinvk("printf", float, d); 
	}

}
