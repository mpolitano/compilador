/*test valido, pasa la hora a formato am pm*/

class pm{

	int horaEnAMPM (int hora){
		if ((hora>12) && (hora<25)) {
			hora-=12;
			return hora;		
		}else{
			if(hora>24){
				return 99;
			}else{
			return hora;
			}
		}
	}

	int main(){
		int a;
		int b;
		a=11;
		b=15; 
		externinvk("printf", int, horaEnAMPM(a));
		externinvk("printf", int, horaEnAMPM(b));	

		return 1;
	}

}
